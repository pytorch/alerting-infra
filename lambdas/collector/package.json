{
  "name": "collector-lambda",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=node20 --format=cjs --outfile=dist/index.js --sourcemap",
    "build:test": "esbuild src/*.ts src/transformers/*.ts --platform=node --target=node20 --format=cjs --outdir=dist --sourcemap",
    "lint": "tsc --noEmit",
    "format": "prettier -w .",
    "format:check": "prettier -c ."
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.137",
    "@types/jsonwebtoken": "^9.0.6",
    "esbuild": "^0.21.5",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.620.0",
    "@aws-sdk/client-secrets-manager": "^3.620.0",
    "@aws-sdk/lib-dynamodb": "^3.620.0",
    "jsonwebtoken": "^9.0.2"
  }
}

{
  "name": "collector-lambda",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "node esbuild.config.js",
    "build:test": "esbuild src/*.ts src/transformers/*.ts --platform=node --target=node20 --format=cjs --outdir=dist --sourcemap",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "tsc --noEmit",
    "format": "prettier -w .",
    "format:check": "prettier -c ."
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.620.0",
    "@aws-sdk/client-secrets-manager": "^3.620.0",
    "@aws-sdk/lib-dynamodb": "^3.620.0",
    "@types/aws-lambda": "^8.10.137",
    "@types/jsonwebtoken": "^9.0.6",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "aws-sdk-client-mock": "^4.0.0",
    "esbuild": "^0.21.5",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "jsonwebtoken": "^9.0.2"
  }
}
